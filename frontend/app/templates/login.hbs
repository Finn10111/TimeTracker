{{outlet}}
<BsForm @model= {{this}} @onSubmit={{action "authenticate"}} @formLayout="vertical" as |form|>
  <form.element @controlType="text" @label="Username" @placeholder="Username" @property="identification" @required=true />
  <form.element @controlType="password" @label="Password" @placeholder="Password" @property="password" @required=true />
  <form.submitButton>Submit</form.submitButton>
    <BsButton @buttonType="submit" @type="primary" @disabled={{form.isSubmitting}}>Submit</BsButton>
</BsForm>

