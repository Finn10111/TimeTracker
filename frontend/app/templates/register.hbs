{{#bs-form model=this onSubmit=(action "register") as |form|}}
  {{form.element controlType="text" label="Username" placeholder="Username" property="identification" required=true}}
  {{form.element controlType="password" label="Password" placeholder="Password" property="password" required=true}}
  {{form.element controlType="password" label="Password (again)" placeholder="Password" property="password_again" required=true}}
  {{bs-button defaultText="Register" type="primary" buttonType="submit"}}
{{/bs-form}}

